{
  "name": "dice-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start:api": "node dist/api/index.js",
    "start:service": "node dist/service/index.js",
    "dev:api": "ts-node src/api/index.ts",
    "dev:service": "ts-node src/service/index.ts",
    "dev:api:log": "nodemon --watch 'src/**/*.ts' --exec ts-node src/api/index.ts",
    "dev:service:log": "nodemon --watch 'src/**/*.ts' --exec ts-node src/service/index.ts",
    "dep:api": "docker-compose -f docker-compose.api.yml up --build -d",
    "dep:service": "docker-compose -f docker-compose.service.yml up --build -d",
    "dep:api:log": "docker-compose -f docker-compose.api.yml up --build",
    "dep:service:log": "docker-compose -f docker-compose.service.yml up --build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.18.2",
    "axios": "^0.20.0",
    "bignumber.js": "^9.0.1",
    "dotenv": "^8.2.0",
    "kafkajs": "^1.14.0",
    "mongodb": "^3.6.2",
    "tron-tx-decoder": "^2.0.2",
    "tronweb": "^3.1.0"
  },
  "devDependencies": {
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.11.8",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
